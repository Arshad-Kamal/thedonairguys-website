{
  "version": 3,
  "routes": [
    {
      "src": "^(?:/(.*))$",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "Permissions-Policy": "accelerometer=(), camera=(), geolocation=(), gyroscope=(), magnetometer=(), microphone=(), payment=(), usb=()"
      },
      "continue": true
    },
    {
      "src": "^/_app/immutable(?:/(.*))$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.js$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.css$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.woff$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.woff2$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.ttf$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.otf$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.ico$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.png$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.jpg$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.jpeg$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.webp$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))\\.svg$",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
      },
      "continue": true
    },
    {
      "src": "/env-test",
      "dest": "/env-test/"
    },
    {
      "src": "/env-test/",
      "status": 308,
      "headers": {
        "Location": "/env-test"
      }
    },
    {
      "src": "/_app/immutable/.+",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/robots\\.txt$",
      "dest": "/api/robots",
      "check": true
    },
    {
      "src": "^/sitemap\\.xml$",
      "dest": "/api/sitemap",
      "check": true
    },
    {
      "src": "/.*",
      "dest": "/![-]/catchall"
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "overrides": {
    "index.html": {
      "path": ""
    },
    "env-test.html": {
      "path": "env-test"
    }
  },
  "crons": []
}
